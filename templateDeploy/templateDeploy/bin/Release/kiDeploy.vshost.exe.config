<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings file="">
    <add key ="srcLoc" value="[location of templates to be deployed]"/>
    <add key="tarLoc" value="[location where templates should be copied]"/>
    <add key="recipients" value="[who to notify of the update]"/>
    <add key="subject" value="Template Updates "/>
    <add key="body" value="The attached templates have been updated and copied to [target].  They are ready to be deployed.  Thanks!"/>
  </appSettings>
</configuration>